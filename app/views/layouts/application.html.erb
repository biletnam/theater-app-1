<!DOCTYPE html>
<html>
  <head>
    <title>TheaterApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="wrapper">
      <div id="panel-left"></div>
  	  <div id="box-left"><%= image_tag('box-left.png', alt: "") %></div>
  	  <div id="box-right"><%= image_tag('box-right.png', alt: "") %></div>
  	  <div id="panel-right"></div>

      <div id="popcorn"></div>
      <div id="content">
        <%= yield %>
      </div>
    </div>
  </body>
</html>
